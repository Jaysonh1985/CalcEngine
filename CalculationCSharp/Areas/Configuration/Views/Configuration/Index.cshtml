@model IEnumerable<CalculationCSharp.Areas.Configuration.Models.ConfigViewModel>
@using System.Linq
@{
    ViewBag.Title = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<html>
<head>
    <link rel='stylesheet prefetch' href='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/themes/smoothness/jquery-ui.css'>
    <link rel='stylesheet prefetch' href='http://netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css'>
    <link href="~/Content/style.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
</head>
<body id="test">
    <div class="container">

        <div id="table" class="table-editable">
            @{ Html.RenderPartial("_Table", Model);}
        </div>

        <button id="export-btn" class="btn btn-primary">Export Data</button>
        <p id="export"></p>
        <input type="text" name="search_name" id="search_name">
        <span style="display:none;" id="search_result"></span>
    </div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src='http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js'></script>
    <script src='http://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js'></script>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.6.0/underscore.js'></script>
    <script src="~/Scripts/Configuration/Configuration.js"></script>

</body>
</html>
